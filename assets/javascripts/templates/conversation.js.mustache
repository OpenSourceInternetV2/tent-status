<div class="row">&nbsp;</div>
<div class="row">
  <div class="span2">&nbsp;</div>
  <div class="span8">
    <ul class="unstyled posts">{{#parent_posts}}{{> _post}}{{/parent_posts}}</ul>
  </div>
</div>
<div class="row">
  <div class="span2">&nbsp;</div>
  <div class="span8 well">
    <ul class="unstyled posts">
      {{#post}}
      <li class="post clearfix" data-id="{{id}}">{{> _post_inner}}</li>
      {{/post}}
    </ul>
  </div>
</div>
<div class="row">
  <div class="span2">&nbsp;</div>
  <div class="span8">
    <ul class="unstyled posts" data-view='ConversationPostsFeed'>
      {{#posts}}{{> _post}}{{/posts}}
    </ul>
  </div>
</div>
